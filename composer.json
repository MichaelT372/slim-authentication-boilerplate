{
    "name": "Slim Authentication boilerplate",
    "description": "Boilerplate for an app with slim php that has authentication with sessions",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://mthomasdev.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Thomas",
            "email": "michaelthomas372@gmail.com",
            "homepage": "http://mthomasdev.com"
        }
    ],
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.3",
        "illuminate/database": "^5.5",
        "respect/validation": "^1.1",
        "slim/csrf": "^0.8.2",
        "slim/flash": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public public/index.php"
    }
